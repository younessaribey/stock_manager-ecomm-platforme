{
  "name": "stock-management-app",
  "version": "1.0.0",
  "description": "Full Stack Stock Management Application with React and Express",
  "main": "index.js",
  "scripts": {
    "client": "cd client && npm start",
    "server": "cd nestjs-backend && npm run start:dev",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "install-all": "npm install && cd client && npm install && cd ../nestjs-backend && npm install",
    "build": "cd client && npm run build",
    "build:client": "cd client && npm run build",
    "build:server": "cd nestjs-backend && npm run build",
    "start": "cd nestjs-backend && npm run start:prod",
    "test": "cd nestjs-backend && npm test",
    "vercel-build": "cd client && npm install && npm run build"
  },
  "keywords": [
    "react",
    "express",
    "stock",
    "inventory",
    "management"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^7.6.0"
  },
  "dependencies": {
    "dotenv": "^16.5.0",
    "pg": "^8.15.5"
  }
}
